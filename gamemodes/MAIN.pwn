			case 1..9: SetPlayerWeather ( playerid, 4 ), SetPlayerTime ( playerid, 18, 0 ) ;
			case 10:SetPlayerTime ( playerid, 12, 0 ), SetPlayerWeather ( playerid, global_weather ) ;
