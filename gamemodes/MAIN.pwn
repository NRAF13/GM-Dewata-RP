}

stock show_doors ( playerid )
{
	new veh_id = GetPlayerVehicleID ( playerid ) ;

	format ( global_string, 216, "\
	"c_server"1. %s "c_white"pintu pengemudi\n\
	"c_server"2. %s "c_white"pintu depan kanan\n\
	"c_server"3. %s "c_white"pintu belakang kiri\n\
	"c_server"4. %s "c_white"pintu belakang kanan",
	( veh_info [ veh_id - 1 ] [ v_door_status ] [ 0 ] ) ? (""c_orange_red"Menutup") : (""c_green"Buka"),
	( veh_info [ veh_id - 1 ] [ v_door_status ] [ 1 ] ) ? (""c_orange_red"Menutup") : (""c_green"Buka"),
	( veh_info [ veh_id - 1 ] [ v_door_status ] [ 2 ] ) ? (""c_orange_red"Menutup") : (""c_green"Buka"),
	( veh_info [ veh_id - 1 ] [ v_door_status ] [ 3 ] ) ? (""c_orange_red"Menutup") : (""c_green"Buka") ) ;
	show_dialog ( playerid, d_carpanel_2, DIALOG_STYLE_LIST, ""c_server"Kontrol pintu mobil", global_string, "Pilih", "Kembali ke" ) ;
	return 1;
}

stock show_contacts ( playerid )
{
	new string [ 738 ], str [ 66 ] ;
	strcat ( string, ""c_server"Nama Kontak:\t\t"c_server"Nomor telepon:\n")
	strcat ( string, ""c_grey"Buku telepon:" ) ;
	for ( new i; i < 10; i++ )
	{
		if ( ! strcmp ( phone_info [ playerid ] [ i ], "-", true ) ) continue ;
		format ( str, sizeof ( str ), "\n"c_white"%s\t\t"c_server"%d telp.", phone_info [ playerid ] [ i ], p_info [ playerid ] [ contact_num ] [ i ] ) ;
		strcat ( string, str ) ;
	}
	SetPVarInt ( playerid, "phone_dialog", 3 ) ;
	return show_dialog ( playerid, d_phone_choose, DIALOG_STYLE_TABLIST_HEADERS, ""c_server"Daftar kontak", string, "Pilih", "Kembali ke" ) ;
}

stock show_complaints ( playerid )
{
	new string [ 56 ] ;
	format ( string, 56, "SELECT `u_fromname`, `u_toname` FROM `users_complaints`" ) ;
	mysql_query ( sql_connection, string ) ;
	new fromname [ MAX_PLAYER_NAME ], toname [ MAX_PLAYER_NAME ], str [ 68 ], rows, fields ;
	cache_get_data ( rows, fields ) ;
	if ( ! rows ) return SendClientMessage ( playerid, col_white, !""scm_error"Tidak ada yang ditemukan." ) ;
	global_string [ 0 ] = EOS ;
	strcat ( global_string, ""c_white"Dari pemain:\t"c_white"Per pemain:" ) ;
	for ( new i; i < rows; i++ )
	{
		cache_get_field_content ( i, "u_fromname", fromname, sql_connection, MAX_PLAYER_NAME ) ;
