		UpdateDynamic3DTextLabelText(b_info[b-1][b_label], -1, query_string);
	}
	else if(b_info[b-1][b_type] == bizz_type_ammo)
	{
		format(query_string, sizeof(query_string), ""c_server"%s\n"c_grey"Harga: "c_white"%d$\n"c_grey"Sebuah keluarga: "c_white"%s\n"c_grey"Bisnis untuk dijual", b_types[b_info[b-1][b_type]], b_info[b-1][b_price], family_info[b_info[b-1][b_family] - 1][fam_name]);
