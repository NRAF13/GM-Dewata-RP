cmd:spcar(playerid, params[])
{
	if ( p_info [ playerid ] [ admin ] < 4 )return SendClientMessage ( playerid, col_gray, ""c_orange_red"* "c_grey"Otoritas tidak memadai." ) ;
	if ( sscanf ( params, "d", params[0] ) )return SendClientMessage ( playerid, col_gray,""c_orange_red"* "c_grey"Menggunakan: /spcar [radius]");
	if ( params [ 0 ] < 1 || params [ 0 ] > 5000) return SendClientMessage ( playerid, col_gray, ""c_orange_red"* "c_grey"Dilarang di bawah 1 dan lebih dari 5000.");

	for(new i = 0; i < MAX_VEHICLES; i++)
	{
		if ( ! IsValidVehicle ( i ) ) continue ;
		if ( GetVehicleModel ( i ) == 450 ) continue ;

		new Float:v_pos_x,
			Float:v_pos_y,
			Float:v_pos_z ;
		GetVehiclePos ( i, v_pos_x, v_pos_y, v_pos_z ) ;

		if ( ! IsPlayerInRangeOfPoint ( playerid, params[0], v_pos_x, v_pos_y, v_pos_z ) ) continue ;
		SetVehicleToRespawn ( i );
	}
	new _t_string [ 120 ] ;
	format ( _t_string, sizeof ( _t_string ), ""c_server"* A.INFO: "c_grey"Administrator %s transportasi yang diperbarui dalam radius %d m.", p_info [ playerid ] [ name ], params [ 0 ] ) ;
 	foreach(new i: admin_players)SendClientMessage ( i, col_gray, _t_string ) ;
	return 1;
}

