stock set_world ( playerid, world )
{
	p_t_info [ playerid ] [ p_data ] [ 1 ] = world ;
	if ( p_t_info [ playerid ] [ p_logged ] == true && GetPVarInt ( playerid, "exam_vehicle" ) == 0 && is_drug_effect { playerid } == 0)
	{
		switch(world)
		{
			case 0:
