		}
		if ( gang_player ( playerid ) && gang_player ( damagedid ) && p_info [ playerid ] [ member ] == p_info [ damagedid ] [ member ] && duel_opponent [ playerid ] == INVALID_PLAYER_ID && duel_opponent [ damagedid ] == INVALID_PLAYER_ID && TeamPaint [ playerid ] == 0 )
