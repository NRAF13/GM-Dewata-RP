cmd:id(playerid, params[])
{
	new cmd_name [ MAX_PLAYER_NAME ],
		pl_id = 0,
		scm_string [ 52 ] ;

    if(sscanf(params,"s[24]",cmd_name) ) return SendClientMessage ( playerid, col_white, !""scm_info"Menggunakan: /id [Nama panggilan pemain]" ) ;
	SendClientMessage ( playerid, col_lblue, !" :" ) ;

	if ( ! isNumeric ( cmd_name ) )
	{
	    new targetid = strval(cmd_name) ;
		if ( ! IsPlayerConnected ( targetid ) ) return SendClientMessage ( playerid, col_white, !""scm_error"Tidak ada yang cocok." ) ;
		format ( scm_string, sizeof(scm_string), "%s (ID: %d) %s", p_info [ targetid ] [ name ], targetid, ( pl_afk_time [ playerid ] > 0 ) ? ( ""c_orange_red"[AFK]"c_white"" ) : ( "" ) ) ;
	    SendClientMessage ( playerid, -1, scm_string ) ;
	    return 1;
	}
	foreach(new i: Player)
	{
		if ( !IsPlayerConnected ( i ) ) continue;
		if ( strfind ( p_info [ i ] [ name ], cmd_name, true ) != -1 )
		{
			if ( pl_id > 5 ) break ;
			format ( scm_string, sizeof(scm_string), "%s (ID: %d) %s", p_info [ i ] [ name ], i, ( pl_afk_time [ playerid ] > 0 ) ? ( ""c_orange_red"[AFK]"c_white"" ) : ( "" ) ) ;
			SendClientMessage ( playerid, -1, scm_string ) ;
			pl_id ++ ;
		}
	}
	return 1;
}

