	format(text_string, sizeof(text_string), "{%s}[Sebuah keluarga] %s %s[%d] diusir dari keluarga %s[%d]. Alasannya: %s", family_info[family_id - 1][fam_chat_color], family_rank[family_id - 1][p_info[playerid][family_rang] - 1], p_info[playerid][name], playerid, p_info[targetid][name], targetid, reason);
	family_message(family_id, col_white, text_string);

	Iter_Remove(family_players[family_id], targetid);
	p_info[targetid][family] = p_info[targetid][family_rang] = 0;

	if(p_info[targetid][family_text] != Text3D:INVALID_3DTEXT_ID)
	{
		DestroyDynamic3DTextLabel(p_info[targetid][family_text]);
		p_info[targetid][family_text] = Text3D:INVALID_3DTEXT_ID;
	}

	new query_string[120];
	format(query_string, sizeof(query_string), "UPDATE `users` SET `u_family`='0',`u_family_rank`='0' WHERE `u_id`='%d' LIMIT 1", p_info[targetid][id]);
	mysql_query(sql_connection, query_string);

	family_info[family_id - 1][fam_members]--;

	format(query_string, sizeof(query_string), "UPDATE `family` SET `fam_members`='%d' WHERE `fam_id`='%d' LIMIT 1", family_info[family_id - 1][fam_members], family_id);
	mysql_query(sql_connection, query_string);

	format(query_string, sizeof(query_string), "%s    %s", p_info[playerid][name], p_info[targetid][name]);
	write_family(playerid, family_id, TYPE_LOG_UVAL, query_string);
	return 1;
}

cmd:famrank(playerid, params[])
{
    if(!p_info[playerid][family]) return SendClientMessage(playerid, col_white, !""scm_error"Anda bukan anggota keluarga.");
    if(p_info[playerid][family_rang] < family_info[p_info[playerid][family] - 1][fam_settings][2])
    {
        new text_string[100];
        format(text_string, sizeof(text_string), ""scm_error"Perintah tersedia dari status %s (%d).", family_rank[p_info[playerid][family] - 1][family_info[p_info[playerid][family] - 1][fam_settings][2] - 1], family_info[p_info[playerid][family] - 1][fam_settings][2]);
        return SendClientMessage(playerid, col_white, text_string);
    }
    extract params -> new player:targetid; else return SendClientMessage(playerid, col_gray, !"Menggunakan: /famrank [id pemain]");
    if(!IsPlayerConnected(targetid) || targetid == playerid) return SendClientMessage(playerid, col_white, !""scm_error"Pemain tidak ditemukan.");
    if(p_info[targetid][family] != p_info[playerid][family]) return SendClientMessage(playerid, col_white, !""scm_error"Pemain itu tidak ada di keluarga Anda.");
    if(p_info[targetid][family_rang] >= family_info[p_info[playerid][family] - 1][fam_settings][3]) return SendClientMessage(playerid, col_white, !""scm_error"Anda tidak dapat mengubah posisi pencipta keluarga.");

    dlobal_string[0] = EOS;
    for(new i = 0; i < family_info[p_info[playerid][family] - 1][fam_settings][3]; i ++)
    {
        new text_string[80];
        format(text_string, sizeof(text_string), ""c_server"%d | "c_white"%s\n", i + 1, family_rank[p_info[playerid][family] - 1][i]);
        strcat(dlobal_string, text_string);
    }
    show_dialog(playerid, d_family_rank, DIALOG_STYLE_LIST, !""scm_dialog"Status pernikahan", dlobal_string, !"Pilih", !"Kembali ke");
    return SetPVarInt(playerid, !"pl_setrank_id", targetid);
}

