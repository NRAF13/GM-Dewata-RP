callback: order_endaccept_callback ( playerid )
{
	new rows,
		fields ;
	cache_get_data ( rows, fields ) ;

	if ( player_rentcar [ playerid ] == INVALID_VEHICLE_ID || player_rentcar [ playerid ] != player_vehicle [ playerid ] ) return is_gps_used { playerid } = 0 ;


	if ( rows )
	{
