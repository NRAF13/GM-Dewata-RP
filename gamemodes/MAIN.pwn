}

CMD:templeader(playerid, params[])
{
	if(p_info[playerid][admin] < 4) return 1;
	if(sscanf(params, "d", params[0])) return SendClientMessage(playerid, col_white, !""scm_info"Menggunakan: /templeader [nomor organisasi]");
	if(params[0] < 0 || params[0] > fraction_count) return SendClientMessage(playerid, col_white, !""scm_error"Nomor organisasi salah.");

	if(params[0] != 0)
	{
		new scm_string[144];
		params[0] -= 1;
		format(scm_string, sizeof scm_string, ""c_server"* A.INFO:"c_grey" %s mengangkat dirinya sendiri sebagai pemimpin sementara organisasi \""c_white"%s"c_grey"\"", p_info[playerid][name], f_info[params[0]][f_name]);
		foreach(new i: admin_players)SendClientMessage(i, col_white, scm_string);


		p_info[playerid][member] = p_info[playerid][leader] =  params[0]+ 1;
		p_info[playerid][rank] = f_info[params[0]][f_rank_settings][0];
		p_info[playerid][org_skin] = f_skin[params[0]][p_info[playerid][gender]][0]	;
	}
	else if(params[0] == 0 && p_info[playerid][leader] > 0)
	{
		new scm_string[230];
		format(scm_string, 144, ""c_server"* A.INFO:"c_grey" %s melepaskan dirinya dari posisi pemimpin sementara organisasi \""c_white"%s"c_grey"\"", p_info[playerid][name], f_info[p_info[params[0]][member] - 1][f_name]);
		foreach(new i: admin_players)SendClientMessage(i, col_white, scm_string);

		p_info[playerid][member] = 0;
		p_info[playerid][leader] = 0;
		p_info[playerid][rank] = 0;

		SetPlayerSkin(playerid, p_info[playerid][skin]);
		SetPlayerColor(playerid, 0xFFFFFF40);
	}
	return 1;
}

