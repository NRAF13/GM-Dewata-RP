				SendClientMessage(playerid, col_gray,""c_orange_red"* "c_grey" Anda telah ditendang karena mencoba "c_orange_red"Team-kill"), kick_player ( playerid ) ;
			}
			ApplyAnimation(playerid,"FAT","IDLE_tired",3.0,1,0,0,0,3000,1);
		}
	}
	if ( p_info [ playerid ] [ jailed ] > 0 )
	{
		anti_dm_warning { playerid } ++ ;
		show_dialog ( playerid, d_none, DIALOG_STYLE_MSGBOX, ""c_server"Peringatan",""c_white"Dilarang bertarung di tempat ini/menembak.\n\n"c_orange_red"* Jika terjadi pelanggaran berulang, Anda bisa ditendang.", "Menerima", "" ) ;
		if ( anti_dm_warning { playerid } >= 5 ) SendClientMessage(playerid, col_gray,""c_orange_red"* "c_grey" Anda telah ditendang karena mencoba "c_orange_red"DM"c_grey" di tempat umum."), kick_player ( playerid ) ;
		ApplyAnimation(playerid,"FAT","IDLE_tired",3.0,1,0,0,0,3000,1);
		return 0;
	}
	if ( weaponid == 9 )
	{
		if ( IsPlayerInRangeOfPoint ( playerid, 150.0, -1092.1349, -1643.6737, 76.373 ) && ! IsPlayerInRangeOfPoint ( playerid, 5, tree_positions [ GetPVarInt(playerid, #Derevo) ] [ 0 ], tree_positions [ GetPVarInt(playerid, #Derevo) ] [ 1 ],tree_positions [ GetPVarInt(playerid, #Derevo) ] [ 2 ] ) )
		{
			anti_dm_warning { playerid } ++ ;
			show_dialog ( playerid, d_none, DIALOG_STYLE_MSGBOX, ""c_server"Peringatan",""c_white"Dilarang menggunakan gergaji mesin untuk keperluan lain.\n\n"c_orange_red"* Jika terjadi pelanggaran berulang, Anda bisa ditendang.", "Menerima", "" ) ;
			if ( anti_dm_warning { playerid } >= 5 ) SendClientMessage(playerid, col_gray,""c_orange_red"* "c_grey" Anda telah ditendang karena mencoba "c_orange_red"DM"c_grey" di tempat umum."), kick_player ( playerid ) ;
			return 0;
		}
	}
    if ( ( ( 22 <= weaponid <= 34) || weaponid == 38 ) )
	{
		#if defined debug_mode
			printf ( "22 <= weaponid <= 34) || weaponid == 38" ) ;
		#endif
	    if ( IsPlayerStreamedIn(damagedid, playerid) && IsPlayerStreamedIn(playerid, damagedid ) )
		{
			if ( pl_afk_time [ playerid ] > 2 )return 1 ;
	  		if ( player_killed [ playerid ]) return 0;


	  		if ( !ProxDetectorS ( 150.0, playerid, damagedid ) ) return 1;

	  		if ( player_legal_gun[ playerid ] == weaponid && player_legal_damage[ playerid ] == damagedid)
			{


			    switch(weaponid)
