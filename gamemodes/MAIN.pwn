callback: makeadmin_callback(playerid, targetid, admin_level)
{
	new rows, fields ;
	cache_get_data ( rows, fields ) ;

	if(admin_level >= p_info[playerid][admin]) return SendClientMessage(playerid, col_white, !""scm_error"Anda tidak dapat mengubah level administrator ini.");

	if(!rows)
	{
		if(admin_level == 0) return SendClientMessage(playerid, col_white, !""scm_error"Pemain bukan administrator.");
		else
		{
			if(p_info[playerid][admin] == admin_level) return SendClientMessage(playerid, col_white, !""scm_error"Anda tidak bisa memberikan level yang sama seperti milik Anda.");

			new query_string[144];
			format(query_string, sizeof(query_string), "INSERT INTO `users_admins`(`u_a_name`,`u_a_level`,`u_a_date`) VALUES ('%s','%d', NOW())", p_info[targetid][name], admin_level);
			mysql_query(sql_connection, query_string);

			format(query_string, sizeof(query_string), ""scm_info"Anda menunjuk "c_server"%s"c_white" di %d tingkat administrator", p_info[targetid][name], admin_level);
			SendClientMessage(playerid, col_white, query_string);

			format(query_string, sizeof(query_string), ""scm_info"Administrator "c_server"%s"c_white" menunjukmu untuk %d tingkat administrator", p_info[playerid][name], admin_level);
			SendClientMessage(targetid, col_white, query_string);
			SendClientMessage(targetid, col_white, !""scm_info"Menggunakan "c_gold"/alogin"c_white" untuk otorisasi di panel admin");
		}
	}
	else
	{
		if(admin_level == 0)
		{
			new query_string[148];
			format(query_string, sizeof(query_string), "DELETE FROM `users_admins` WHERE `u_a_name`='%s' LIMIT 1", p_info[targetid][name]);
			mysql_query(sql_connection, query_string);

			if(p_info[targetid][admin] != 0)
			{
				p_info[targetid][admin] = 0;
				Iter_Remove(admin_players, targetid);
			}

			format(query_string, sizeof(query_string), ""scm_info"Anda lepas landas "c_server"%s"c_white" dari pos administrator", p_info[targetid][name]);
			SendClientMessage(playerid, col_white, query_string);
		}
		else
		{
			if(p_info[playerid][admin] == admin_level) return SendClientMessage(playerid, col_white, !""scm_error"Anda tidak bisa memberikan level yang sama seperti milik Anda.");

			new query_string[128];
			format(query_string, sizeof(query_string), "UPDATE `users_admins` SET `u_a_level`='%d' WHERE `u_a_name`='%s' LIMIT 1", admin_level, p_info[targetid][name]);
			mysql_query(sql_connection, query_string);

			p_info[targetid][admin] = admin_level;

			format(query_string, sizeof(query_string), ""scm_info"Anda menunjuk "c_server"%s"c_white" di %d tingkat administrator", p_info[targetid][name], admin_level);
			SendClientMessage(playerid, col_white, query_string);

			format(query_string, sizeof(query_string), ""scm_info"Administrator "c_server"%s"c_white" menunjukmu untuk %d tingkat administrator", p_info[playerid][name], admin_level);
			SendClientMessage(targetid, col_white, query_string);
		}
	}
	return 1;
}


