			{
				new cp_id = GetPVarInt ( playerid, "miner_cp" ) ;

				if ( IsPlayerInRangeOfPoint ( playerid, 3.5, miner_cp [ cp_id ] [ 0 ], miner_cp [ cp_id ] [ 1 ], miner_cp [ cp_id ] [ 2 ] ) )//  
