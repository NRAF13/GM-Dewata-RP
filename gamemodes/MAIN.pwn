	if(!IsPlayerConnected(targetid) || targetid == playerid) return SendClientMessage(playerid, col_gray, !"Pemain tidak ditemukan.");
	if(p_info[targetid][admin] >= p_info[playerid][admin]) return SendClientMessage(playerid, col_gray, !"Anda tidak dapat mengubah level admin dari administrator ini");

	SetPVarInt(playerid, !"target_id", targetid);

	new header_string[45], body[13];
	global_string[0] = EOS;
	global_string = !""c_grey"Hapus dari postingan administrator\n"c_white"";

	for(new i = 1; i < p_info[playerid][admin]; i++)
	{
		valstr(body, i);
		strcat(body, " \n");
		strcat(global_string, body);
	}

	format(header_string, sizeof(header_string), ""scm_dialog"%s", p_info[targetid][name]);
	return show_dialog(playerid, d_makeadmin, DIALOG_STYLE_LIST, header_string, global_string, !"Lebih lanjut", !"Pembatalan");
